{
  "name": "mac-portfolio",
  "private": true,
  "description": "Simple portfolio site using Gatsby and DatoCMS",
  "version": "0.1.0",
  "author": "Mac Radinoff",
  "dependencies": {
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.50",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-manifest": "^2.4.24",
    "gatsby-plugin-offline": "^3.2.24",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.28",
    "gatsby-source-datocms": "^2.4.0",
    "gatsby-source-filesystem": "^2.3.25",
    "gatsby-transformer-sharp": "^2.5.13",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "require-dir": "^1.2.0"
  },
  "devDependencies": {
    "prettier": "2.1.0"
  },
  "keywords": [
